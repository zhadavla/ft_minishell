minishell$ echo "hello"
hello
minishell$ echo "world"
world
minishell$ echo "hello world"
hello world
minishell$ echo "hello'world'"
hello'world'
minishell$ echo "hello $USER"
hello vlad
minishell$ echo "hello $USER from $PWD"
hello vlad from /home/vlad/42/ft_minishell/test
minishell$ echo "hello $USER from $HOME"
hello vlad from /home/vlad
minishell$ echo "hello $USER from '$HOME'"
hello vlad from '/home/vlad'
minishell$ echo "hello '$USER from' $HOME"
hello 'vlad from' /home/vlad
minishell$ echo '"hello $USER from $HOME"'
"hello $USER from $HOME"
minishell$ exit
